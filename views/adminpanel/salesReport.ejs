  <style>
    .sales-report {
        width: 100%;
        border-collapse: collapse;
       }
       
       .sales-report th, .sales-report td {
        padding: 15px;
        text-align: left;
        border-bottom: 1px solid #ddd;
       }
       
       .sales-report tr:hover {background-color: #f5f5f5;}
       
  </style>
