              <span class="menu-title">Discounts</span>
              <i class="menu-arrow"></i>
            </a>
           
          </li> -->
        
          
        </ul>
      </nav>
      <!-- partial -->
      <div class="main-panel">
        <div class="content-wrapper">
          <div class="row">
            <div class="col-sm-12">
              <div class="home-tab">
                
                <div class="tab-content tab-content-basic">
                  <div class="tab-pane fade show active" id="overview" role="tabpanel" aria-labelledby="overview"> 
                    
                     
                    <div class="row">
                   
                            <div class="card card-rounded">
                              <div class="card-body">
                                <div class=" justify-content-between align-items-start">
                                  <div class="table-responsive cart_info">
                                    <div >
                                       <h2>Update products</h2>
                                       <form action="/admin/productUpdated/<%= productToUpdate._id%>" method="post">
                                           <p>
                                            <label for="productName">Product Name</label>      
                                            <input type="text" class="form-control" name="productName" value="<%=productToUpdate.productName %>" >
                                           </p>
                                          
                                           

                                           <p>
                                             <label for="price">Price</label>
                                             <input type="number" class="form-control" name="price" value="<%=productToUpdate.price %>">
                                           </p>
                                           
                                                  
                                             
                                            <p>
                                            <label for="stock">stock</label>
                                            <input type="text" class="form-control" name="stock" value="<%=productToUpdate.stock %>">
                                            </p>  
                                              

                                            <p>
                                                <label for="image">Image Url</label>
                                                <input type="text" class="form-control" name="image">
                                            </p>

                                            <label for="discription">discription</label>
                                            <input type="text" class="form-control" name="discription" value="<%=productToUpdate.discription %>">
                                           </p>

                                           <input type="submit" name="" id="" value="Update">
                                       </form>
                                    </div>


                                   
                                   
                                  </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                     
                    </div>
                  </div>
